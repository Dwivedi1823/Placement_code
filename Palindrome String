#include <bits/stdc++.h>
using namespace std;

int main() 
{
    string s;
    cin>>s;
    string t;
    for(int i = 0; i < s.size()/2; i++)
    {
        if(s[i] != s[s.size() - i - 1])
        {
            cout<<"Not Pelindrome!";
            return 0;
        }
    }
    cout<<"Pelindrome.";
	return 0;
}
